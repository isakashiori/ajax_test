<%= javascript_pack_tag 'tag' %>
<h1>ポストとタグ付けテスト</h1>


<%= form_with model: @post do |f| %>
  <%= f.label :title%> <br>
  <%= f.text_field :title%><br>
  <%= f.label :body%><br>
  <%= f.text_area :body%><br>

  <%= f.label :tag_names %><br>
  <%= f.text_field :tag_names, name:'basic', placeholder: ',で区切って入力してください'  %><br>

  <%= f.submit %>  
<% end %>
  